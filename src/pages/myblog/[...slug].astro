---
import { type CollectionEntry, getCollection } from 'astro:content';
import BlogPost from '../../layouts/MyBlogPost.astro';

export async function getStaticPaths() {
	const posts = await getCollection('myblog');
	return posts.map((post) => ({
		params: { slug: post.slug },
		props: post,
	}));
}
type Props = CollectionEntry<'myblog'>;

const post = Astro.props;
const { Content ,headings} = await post.render();

const testarry=['haha','haha'];
---

<BlogPost {...post.data} teststr='teststr' testarry={testarry} headings={headings}>
	<Content />
</BlogPost>